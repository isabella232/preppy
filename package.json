{
  "name": "prepublish-lib",
  "version": "0.10.1",
  "description": "A solution for simplifying pre-publishing typical JavaScript projects for publishing to NPM.",
  "keywords": [
    "npm",
    "publish",
    "library"
  ],
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=4.0.0",
    "yarn": ">=0.17.0"
  },
  "bin": {
    "prepublish-lib": "./bin/prepublish-lib.js"
  },
  "author": {
    "name": "Sebastian Werner",
    "email": "s.werner@sebastian-software.de",
    "url": "http://sebastian-software.de/werner"
  },
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint --quiet src/ test/ || true",
    "release": "release-it --github.release --npm.publish --non-interactive",
    "release:minor": "release-it --github.release --npm.publish --non-interactive --increment minor",
    "release:major": "release-it --github.release --npm.publish --non-interactive --increment major",
    "test": "rimraf test/lib test/dist && npm run prepublish && node bin/prepublish-lib --verbose --entry-node test/src/index.js --output-folder test/lib && webpack --config test/webpack.config.js && node test/dist/bundle.js",
    "prepublish": "rimraf bin && rollup -c rollup.cfg"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sebastian-software/prepublish-lib.git"
  },
  "dependencies": {
    "async": "^2.1.4",
    "babel-plugin-closure-elimination": "^1.1.14",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-es3-member-expression-literals": "^6.8.0",
    "babel-plugin-transform-es3-property-literals": "^6.8.0",
    "babel-plugin-transform-flow-strip-types": "^6.21.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.11",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-babili": "^0.0.9",
    "babel-preset-env": "^1.1.4",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-runtime": "^6.20.0",
    "file-exists": "^2.0.0",
    "fs-extra": "^1.0.0",
    "loader-utils": "^0.2.16",
    "lodash": "^4.17.2",
    "meow": "^3.7.0",
    "rollup": "^0.37.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-json": "^2.1.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-relink": "^0.3.1",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-yaml": "^1.0.0"
  },
  "devDependencies": {
    "file-loader": "^0.9.0",
    "rimraf": "^2.5.4",
    "s15e-javascript": "^0.7.8",
    "webpack": "^2.2.0-rc.1"
  }
}
