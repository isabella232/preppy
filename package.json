{
  "name": "prepublish-lib",
  "version": "0.1.1",
  "description": "A solution for simplifying pre-publishing typical JavaScript projects for publishing to NPM.",
  "keywords": [
    "npm",
    "publish",
    "library"
  ],
  "engines": {
    "node": "~6.3.0"
  },
  "bin": {
    "prepublish-lib": "./bin/prepublish-lib"
  },
  "author": "Sebastian Werner",
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint --quiet src/ test/ || true",
    "nsp": "nsp check --output summary",
    "release": "release-it --github.release --npm.publish --non-interactive",
    "release:minor": "release-it --github.release --npm.publish --non-interactive --increment minor",
    "release:major": "release-it --github.release --npm.publish --non-interactive --increment major",
    "test": "npm run prepublish && node bin/prepublish-lib",
    "prepublish": "rollup -c rollup.cfg"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sebastian-software/prepublish-lib.git"
  },
  "dependencies": {
    "async": "^2.0.1",
    "denodeify": "^1.2.1",
    "lodash": "^4.15.0",
    "read-package-json": "^2.0.4",
    "rollup": "^0.34.9",
    "rollup-plugin-buble": "^0.13.0",
    "rollup-plugin-commonjs": "^3.3.1",
    "rollup-plugin-node-resolve": "^2.0.0"
  },
  "devDependencies": {
    "s15e-javascript": "^0.2.3"
  }
}
