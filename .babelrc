{
  "comments": false,
  "sourceMaps": true,

  "env": {
    "development": {
      "plugins": [
        [
          "transform-object-rest-spread",
          {
            "useBuiltIns": true
          }
        ],
        [
          "fast-async",
          {
            "useRuntimeModule": true
          }
        ],
        [
          "transform-runtime",
          {
            "helpers": true,
            "regenerator": false,
            "polyfill": false,
            "useBuiltIns": true,
            "useESModules": true
          }
        ]
      ],

      "presets": [
        [ "env", {
          "exclude": [
            "transform-regenerator",
            "transform-async-to-generator"
          ],
          "useBuiltIns": true,
          "loose": true,
          "modules": false,
          "targets": {
            "node": "6.9.0"
          }
        }]
      ]
    },
    "test": {
      "plugins": [
        [
          "transform-object-rest-spread",
          {
            "useBuiltIns": true
          }
        ],
        [
          "fast-async",
          {
            "useRuntimeModule": true
          }
        ]
      ],

      "presets": [
        [ "env", {
          "exclude": [
            "transform-regenerator",
            "transform-async-to-generator"
          ],
          "useBuiltIns": true,
          "loose": true,
          "modules": "commonjs",
          "targets": {
            "node": "current"
          }
        }]
      ]
    }
  }
}
